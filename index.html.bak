<script>
        // Initialize the map and set its view
        var map = L.map('map').setView([49.41260270970647, -123.5690389849176], 11);

        // Add a tile layer (OpenStreetMap)
        L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
            attribution: '&copy; <a href="https://www.openstreetmap.org/copyright">OpenStreetMap</a> contributors'
        }).addTo(map);

        // Disable zoom handlers
        map.touchZoom.disable();
        map.doubleClickZoom.disable();
        map.scrollWheelZoom.disable();

        // Disable tap handler, if present
        if (map.tap) map.tap.disable();
    </script>



  <!-- This file contains the little map markers that can be changed to show different activities or accomodation -->
  <script type="text/javascript" src="js/map-popups.js"></script>

<!-- map end --> 

</body>
</html>